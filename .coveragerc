[run]
source = perplexity_cli
omit =
    */tests/*
    */test_*.py
    */__pycache__/*
    */.venv/*
    */venv/*
    */setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.
    if TYPE_CHECKING:
    @abstractmethod
    @overload

precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
